ssm_sim4$N[1] = N1
ssm_sim4$Nbis[1] = N1
pas = 1
H = 0
sigma = 0.15
K = 800
alpha = 0.5
ite=0
tempH=c()
NAMharvest = 0.15
for (nyears in seq(5,nyears,pas)) { # Boucle sur le nombre d'itérations
ite=ite+1
tempH[ite]=H
if (nyears == 5) {
for (t in 1:(nyears - 1)) {
u = ssm_sim4$N[t]*(1-H)
v = ssm_sim4$Nbis*(1-NAMharvest)
Er = exp(alpha * (1 - c(u,v)/ K)) * c(u,v)
ssm_sim4$N[t+1] = rpois(1,Er[1])
ssm_sim4$Nbis[t+1] = rpois(1,Er[2])
}
}
if(nyears>5){
for (t in (nyears - pas):(nyears - 1)) {
u = ssm_sim4$N[t]*(1-H)
v = ssm_sim4$Nbis[t]*(1-NAMharvest)
Er = exp(alpha * (1 - c(u,v)/ K)) * c(u,v)
ssm_sim4$N[t+1] = rpois(1,Er[1])
ssm_sim4$Nbis[t+1] = rpois(1,Er[2])
}
}
for (t in 1:nyears){
ssm_sim4$y[t]=rpois(1,ssm_sim4$N[t])
ssm_sim4$ybis[t]=rpois(1,ssm_sim4$Nbis[t])
}
